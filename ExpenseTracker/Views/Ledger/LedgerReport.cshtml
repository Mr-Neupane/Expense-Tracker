@model System.Collections.Generic.IEnumerable<dynamic>

@{
    ViewData["Title"] = "Create Ledger";
}

<table class="table table-striped table-bordered table-hover">
    <thead class="thead-dark">
    <tr>
        <th>SN</th>
        <th>COA</th>
        <th>Parent Ledger Name</th>
        <th>Ledger Name</th>
        <th>Status</th>
        <th>Created By</th>
        </tr>
    </thead>
    <tbody>
    @{
        int sn = 1;
    }
    @foreach (var item in Model)
    {
        <tr>
            <td>@sn</td>
            <td>@item.name</td>
            <td>@item.subparentname</td>
            <td>@item.ledgername</td>
            <td>@(item.status == 1 ? "Active" : "Inactive")</td>
            <td>@item.username</td>
        </tr>
        sn++;
    }
    </tbody>
</table>